#include <avr/io.h>
#include <avr/interrupt.h>

ISR(TIMER1_COMPA_vect)
{
	if(PORTC == 0)
		PORTC == 0xFF;
	else
		PORTC == 0;
}


int main()
{
	DDRC = 0xFF;
	DDRD = 0;
	PORTC = 0;
	PORTD = 0xFF;
	TCCR1B |= (1<<3) | (1<<1) | (1<<0); //Ustalanie licznika
	OCR1A = 17280;
	sei();
	while(1){}

}
